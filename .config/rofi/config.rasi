    /* drun-display-format: */
    /*** Command to open a Desktop Entry that is a Link. [string]: xdg-open ***/
    /* drun-url-launcher: */
    /*** Programs ignored for history [string]: ***/
    /* ignored-prefixes: */
    /*** Choose the strategy used for sorting: normal (levenshtein) or fzf. [string]: normal ***/
    /* sorting-method: */
    /*** Set the modi to combine in combi mode [string]: window,run ***/
    combi-modi: "window,drun,run";
