configuration {
    /*** Location on screen [number]: 0 ***/
    /* location: */
    /*** Y-offset relative to location [number]: 0 ***/
    /* yoffset: */
    /*** X-offset relative to location [number]: 0 ***/
    /* xoffset: */
    /*** Row height (in chars) [number]: 1 ***/
    /* eh: */
    /*** DPI [number]: -1 ***/
    /* dpi: */
    /*** Threads to use for string matching [number]: 0 ***/
    /* threads: */
    /*** Scrolling method. (0: Page, 1: Centered) [number]: 0 ***/
    scroll-method: 1;
    /*** Max history size (WARNING: can cause slowdowns when set too high). [number]: 25 ***/
    /* max-history-size: */

    /*** Set the character used to negate the matching. ('\0' to disable) [character]: - ***/
    /* matching-negate-char: */

    /*** Always show number of lines: True ***/
    /* fixed-num-lines: */
    /*** Whether to load and show icons: False ***/
    show-icons: true;
    /*** Desktop entry show actions.: False ***/
    /* drun-show-actions: */
    /*** Disable history in run/ssh: False ***/
    /* disable-history: */
    /*** Use sorting: False ***/
    /* sort: */
    /*** Set case-sensitivity: False ***/
    /* case-sensitive: */
    /*** Cycle through the results list: True ***/
    /* cycle: */
    /*** Enable sidebar-mode: False ***/
    sidebar-mode: true;
    /*** Enable hover-select: False ***/
    /* hover-select: */
    /*** Enable auto select mode: False ***/
    /* auto-select: */
    /*** Parse hosts file for ssh mode: False ***/
    parse-hosts: true;
    /*** Parse known_hosts file for ssh mode: True ***/
    /* parse-known-hosts: */
    /*** Tokenize input string: True ***/
    /* tokenize: */
    /*** Click outside the window to exit: True ***/
    /* click-to-exit: */
    /*** Hide the prefix mode prefix on the combi view.: False ***/
    /* combi-hide-mode-prefix: */
    /*** Show window thumbnail (if available) as icon in window switcher.: False ***/
    window-thumbnail: true;
    /*** DRUN: build and use a cache with desktop file content.: False ***/
    /* drun-use-desktop-cache: */
    /*** DRUN: If enabled, reload the cache with desktop file content.: False ***/
    /* drun-reload-desktop-cache: */
    /*** Normalize string when matching (disables match highlighting).: False ***/
    /* normalize-match: */
    /*** Steal focus on launch and restore to window that had it on rofi start on close .: False ***/
    /* steal-focus: */

    /*** Enabled modi [string]: window,run,ssh ***/
    modi: "drun,run,window,combi";
    /*** Font to use [string]: mono 12 ***/
    font: "System San Francisco Display 12";
    /*** Terminal to use [string]: rofi-sensible-terminal ***/
    /* terminal: */
    /*** Ssh client to use [string]: ssh ***/
    /* ssh-client: */
    /*** Ssh command to execute [string]: {terminal} -e {ssh-client} {host} [-p {port}] ***/
    /* ssh-command: */
    /*** Run command to execute [string]: {cmd} ***/
    /* run-command: */
    /*** Command to get extra run targets [string]: ***/
    /* run-list-command: */
    /*** Run command to execute that runs in shell [string]: {terminal} -e {cmd} ***/
    /* run-shell-command: */
    /*** Command to executed when -kb-accept-alt binding is hit on selected window  [string]: wmctrl -i -R {window} ***/
    /* window-command: */
    /*** Window fields to match in window mode [string]: all ***/
    /* window-match-fields: */
    /*** Theme to use to look for icons [string]: (unset) ***/
    /* icon-theme: */
    /*** Desktop entry fields to match in drun [string]: name,generic,exec,categories,keywords ***/
    /* drun-match-fields: */
    /*** Only show Desktop entry from these categories [string]: (unset) ***/
    /* drun-categories: */
